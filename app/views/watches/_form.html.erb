
<%= simple_form_for @watch do |f| %>
	<%= f.input :name %>
	<%= f.input :description %>
	<%= f.input :price_in_pence %>
	<%= f.input :style %>
	<%= f.input :gender %>
	<%= f.input :brand %>
	<%= f.input :pre_owned %>
	<%= f.input :image, as: :file %>
	<%= f.button :submit, "Upload Watch" %>
<% end %>